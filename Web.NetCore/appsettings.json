{
  "Logging": {
    "LogLevel": {
      "Default": "Trace"
    }
  },
  "AllowedHosts": "*",

  // Settings for MakeMeAPassword.
  "Mmap": {

    "IpThrottler": {
      // Count of requests from an IP address are reset after this many hours.
      "ResetPeriodHours": 1.0,
      // Limit of number of request units (1 password / 1 combinations request) in a single reset period.
      "Limit": 200,
      // Networks which are considered local and are not counted.
      "LocalNetworks": [
        "192.168.0.0/16",
        "10.0.0.0/8"
      ]
    },

    "Terninger": {
      // API keys for 3rd party random generation sites.
      "HotBitsApiKey": "",
      "RandomOrgApiKey": ""
    }

  }
}
